{
  "name": "@uppercod/keen-slider",
  "type": "module",
  "description": "It is a wrapper for keen-slider as a webcomponent created with Atomico",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uppercod/keen-slider.git"
  },
  "version": "0.1.0",
  "scripts": {
    "start": "vite --force",
    "build": "vite build",
    "test": "wtr src/**/*.test.js --node-resolve",
    "exports": "exports src/components.js --exports --analyzer --types --main components",
    "prepublishOnly": "npm run exports"
  },
  "dependencies": {
    "@atomico/hooks": "^3.29.0",
    "atomico": "^1.45.0",
    "keen-slider": "^6.6.3"
  },
  "devDependencies": {
    "@atomico/exports": "^0.22.2",
    "@uppercod/vite-meta-url": "latest",
    "@uppercod/vite-meta-url-load-css": "latest",
    "typescript": "^4.5.5",
    "vite": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./components.react": "./dist/components.react.js",
    "./components.visibility": "./dist/components.visibility.css",
    ".": "./dist/components.js"
  },
  "typesVersions": {
    "*": {
      "components.react": [
        "./types/components.react.d.ts"
      ],
      "components": [
        "./types/components.d.ts"
      ]
    }
  },
  "peerDependencies": {
    "@atomico/react": "latest"
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    }
  },
  "module": "./dist/components.js",
  "types": "types/components.d.ts"
}