    "atomico": "^1.45.0",
    "keen-slider": "^6.6.3"
  },
  "devDependencies": {
    "@atomico/exports": "^0.22.2",
    "@uppercod/vite-meta-url": "latest",
    "@uppercod/vite-meta-url-load-css": "latest",
    "typescript": "^4.5.5",
    "vite": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
